                                                    <th>Route Name(Date Time)</th>
                                                    <th style="display:none;">ID</th>
                                                    <th>Assign Vehicles</th>
                                                    <th>ETA in Minutes</th>
                                                    <th>Distance(km)</th>
                                                    @*<th style="display:none;">Schedule Dispatch Of Vehicles ?</th>*@
                                                    <th>Set Route Multiple Timing ?</th>
                                                    <th>Action</th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    </div>
                </div>
            </div>
        </div>
