<link href="~/Content/css/multi-select.css" rel="stylesheet" />
<link href="~/Content/select2/select2.css" rel="stylesheet" />
<link href="~/Content/select2/select2-bootstrap.css" rel="stylesheet" />
<style type="text/css">
    a.details-control {
        background: url('../Content/img/details_open.png') no-repeat center center;
        cursor: pointer;
    }

    .panel-collapse.shown a.details-control {
        background: url('../Content/img/details_close.png') no-repeat center center;
    }
</style>
<script src="~/Content/select2/select2.min.js"></script>
<div class="row-fluid">
    <div class="widget-body ">
        <div class="main_container" id="tables_page">
            <div class="row-fluid" style="margin-bottom: 25px;">
                <div class="widget-body" style="border: 0">
                    <div class="row-fluid">
                        <div id="alerts" class="widget span12">
                            <div class="widget-header">
                                <i class="fa fa-file-archive-o"></i>
                                <h5 id="statusText">Add Billing Info</h5>
                                <div class="btn-group btn-adjst" style="margin-left: 37px;">
                                </div>
                            </div>
                            <div class="row-fluid" style="margin-bottom: 12px; margin-top: -37px;">
                                <div class="widget-body" style="border: 0">
                                    <div class="row-fluid">
                                        <div id="alertss" class="widget span12">
                                            <div class="widget-body" style="background: #fff; overflow-x: auto">
                                                <div id="DivVehcleCounter">
                                                    <div class="box-content padded">
                                                        <form id="VehDiv1" method="post" class="col-centered">
                                                            @Html.Hidden("VehicleId")
                                                            <div class="form-group col-md-2">
                                                                Select Excel File
                                                                <span class="label label-info">supported filetypes:.xls,.xlsx</span>
                                                            </div>
                                                            <div class="form-group col-md-2">
                                                                @Html.Hidden("ExcelFileName")
                                                                @Html.Hidden("ExcelPath")
                                                                <input type="button" value="Choose File" onclick="document.getElementById('files').click();" /><br />
                                                                <input type="file" id="files" name="files[]" style="display: none;" />
                                                                <br />
                                                            </div>
                                                            <div class="form-group col-md-2">
                                                                <div class="col-sm-4">
                                                                    <input type="submit" value="submit" id="submitdiv1" class="btn btn-green" />
                                                                </div>
                                                            </div>
                                                        </form>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row-fluid" style="margin-bottom: 12px; margin-top: -37px;">
                <div class="widget-body" style="border: 0">
                    <div class="row-fluid">
                        <div id="alertss" class="widget span12">
                            <div class="widget-body clearfix" style="overflow-x: auto">
                                <div class="box" style="background: #fdfdfd;">
                                    <div class="box-content padded">
                                        <div class="row">
                                            <form class="form-inline">
                                                <div class="col-md-3 form-group" style="padding-left: 35px;">
                                                    <input type="text" id="from" placeholder="From City" name="FilterKM">
                                                </div>
                                                <div class="col-md-3 form-group" style="padding-left: 35px;">
                                                    <input type="text" id="to" placeholder="City Destination" name="FilterKM">
                                                </div>
                                                <div class="col-md-3 form-group" style="padding-left: 35px;">
                                                    <input type="text" id="truck" placeholder="Truck No" name="FilterKM">
                                                </div>
                                                <div class="col-md-3 form-group" style="padding-left: 35px;">
                                                    <input type="text" id="grno" placeholder="GR No" name="FilterKM">
                                                </div>
                                                <div class="col-md-4 form-group">
                                                    <input type="button" value="search" id="BtnSearch" class="btn btn-green">
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="widget-body clearfix" style="background: #fff; overflow-x: auto">
                                <div id="DivVehcleCounter">
                                    <div class="box-content padded">
                                        <table id="dt_basic_Master" class="table table-bordered" cellspacing="0" width="100%">
                                            <thead>
                                                <tr>
                                                    <th>S.No</th>
                                                    <th>Despatch Location</th>
                                                    <th>Sold To Party</th>
                                                    <th>Ship To Party City</th>
                                                    <th>Ship To Party Region Desc</th>
                                                    <th>Bill Doc</th>
                                                    <th>Excise Invoice No</th>
                                                    <th>Shipment No</th>
                                                    <th>Billing Date</th>
                                                    <th>Truck No</th>
                                                    <th>Transporter</th>
                                                    <th>GRNO</th>
                                                    <th>Plant</th>
                                                    <th>Division</th>
                                                </tr>
                                            </thead>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--/widget-body-->
            </div>
        </div>
    </div>
</div>

@section scripts{
    <script src="~/Scripts/app/common.js"></script>
    <script src="~/Scripts/app/Test.js"></script>
}