        $(function () {
            $('#tglShowHideFence').change(function () {
                if ($(this).prop('checked')) {
                    var value = getUrlParameter("tableName");
                    GeoFence(value);
                } else {
                    $("#trAlertMessage").addClass("hidden");
                    initialize();
                    iterator = 0;
                    line = [];
                    $("#slider").trigger('change');
