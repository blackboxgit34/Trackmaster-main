    <style type="text/css">
        .panel1
        {
            background: none repeat scroll 0 0 #fff;
            border: 1px solid #ccc;
            height: auto;
            left: 0;
            opacity: 0.85;
            padding: 10px;
            position: absolute;
            top: 80px;
            width: 300px;display:none;
        }
        a.trigger1
        {
            position: absolute;
            text-decoration: none;
            top: 43px;
            left: 0;
