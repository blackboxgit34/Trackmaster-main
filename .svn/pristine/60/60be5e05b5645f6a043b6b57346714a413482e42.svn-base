            data: { custId: $custid },
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                // 
                if (data > 0) {
                    $("#panicAlertDash").text(data);
                    $("#bdgAnyAlert").removeClass("hidden");
                }
            },
            failure: function (response) {
                alert("Error");
            }
        });
