    <script language="JavaScript" type="text/javascript">
        var timerID = null;
        var timerRunning = false;
        var delay = 20000;
        var zoomToExtents = 0;
        var openedInfoWindow = null;
        function initializeTimer() {
            //zoomToExtents = 0;
            initializeMap();
