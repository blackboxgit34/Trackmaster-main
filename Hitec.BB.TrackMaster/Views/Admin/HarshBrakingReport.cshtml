    <!-- /row-fluid -->
</div>
<!-- /Main window -->
<div id="ShowLocationOnMap" title="Basic dialog">
</div>
@section css{
    <style>
        .col-md-2 {
            width: 15.6%;
            /* margin-right: 0px; */
