@{
    ViewBag.Title = "Harsh Braking Report";
}

<link href="~/Content/css/tabsWithAnimation.css" rel="stylesheet" />
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

<div class="row-fluid">
    
        <div class="widget-body " >
            
            <!-- Main window -->
            <div class="main_container" id="tables_page">
                @Html.Action("_ReportFilter","Report", new { reportTitle = "Harsh Braking Report" })
                <br />
                <div class="row-fluid" style="margin-bottom: 12px; margin-top: -37px;">
                    <div class="widget-body" style="border: 0">
                        <div class="row-fluid">
                            <div id="alertss" class="widget span12">

                                <div class="widget-body  " style="background: #fff; overflow-x: auto">
                                    <div id="DivVehcleCounter">
                                        <div class="box-content padded">
                                      
                         
                                            <table id="dt_basic_Master" class="table table-bordered" cellspacing="0" width="100%">
                                                <thead>
                                                    <tr>
                                                       
                                                        <th>Vehicle Name
                                                        </th>
                                                        <th>Driver Name
                                                        </th>
                                                        <th>Harsh Braking Count
                                                        </th>
                                                       <th>
                                                     Rating in (%)
                                                    </th>
