<div class="row-fluid">
    <div class="widget-body ">
        <div class="main_container" id="tables_page">
            <div class="row-fluid" style="margin-bottom: 25px;">
                <div class="widget-body" style="border: 0">
                    <div class="row-fluid">
                        <div id="alerts" class="widget span12">
                            <div class="widget-header">
                                <i class="fa fa-file-archive-o"></i>
                                <h5>Account Summary </h5>
                            </div>
                        </div>
                    </div>
                </div>
                <!--/widget-body-->
            </div>
            <div class="row-fluid" style="margin-bottom: 12px; margin-top: -37px;">
                <div class="widget-body" style="border: 0">
                    <div class="row-fluid">
                        <div id="alertss" class="widget span12">
                            <div class="widget-body clearfix" style="background: #fff; overflow-x: auto">
                                <div id="DivVehcleCounter" style="padding: 15px;">
                                    <div class="box-content padded">
                                        <table id="dt_basic_Master" class="table table-bordered" width="100%">
                                            <thead>
                                                <tr>
                                                    <th>Bill no</th>
                                                    <th>Billing Cycle</th>
                                                    <th>Blackbox</th>
                                                    <th>Tot.subscription</th>
                                                    <th>Prev.Balance [+]</th>
                                                    <th>Payable Amt.</th>
                                                    <th>Balance<br />(with Tax 15%)</th>
                                                    <th>Billing</th>
                                                    <th>Pay Online</th>
                                                </tr>
                                            </thead>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section css{
    <style>
        .col-sm-2 {
            width: 15.6%;
            margin-left: 6px;
            background: #fff;
            text-align: left;
        }

        .box {
            background: #dddcdc none repeat scroll 0 0;
            border: 0px solid #cdcdcd;
            border-radius: 3px;
            box-shadow: none;
            margin-bottom: 5px;
        }
    </style>
}
@section scripts{
    <script>
        var reportName;
        $(document).ready(function () {
            reportName = '@ViewBag.Report';
            $(".js-example-basic").select2({});
        });
    </script>
    <!--common js file-->
    <script src="@Url.Content("~/scripts/app/common.js")" @DateTime.Now.Ticks></script>
    <script src="~/Content/js/APIURL.js"></script>
    <script type="text/javascript" src="@Url.Content("~/scripts/app/AccountSummaryCustom.js?")" @DateTime.Now.Ticks></script>
}
