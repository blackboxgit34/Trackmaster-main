@{
    ViewBag.Title = "GodrejvehList";
}

<div class="row-fluid">
    <div class="widget-body ">
      
        <div class="main_container" id="tables_page">
            @Html.Action("_ReportFilter", "Report", new { reportTitle = "Registered Vehicle Listg" })
            <br />
            <div class="row-fluid" style="margin-bottom: 12px; margin-top: -37px;">
                <div class="widget-body" style="border: 0">
                    <div class="row-fluid">
                        <div id="alertss" class="widget span12">
                            <div class="widget-body clearfix" style="background: #fff; overflow-x: auto">
                                <div id="DivVehcleCounter">
                                    <div class="box-content padded">
                                        <table id="dt_basic_Master" class="table table-bordered" cellspacing="0" width="100%">
                                            <thead>
                                                <tr>
                                                    <th>TagID</th>
                                                    <th>Vehicle No</th>
                                                    <th>Company Name</th>
                                                    <th>Owner Name </th>
                                                    <th>Owner Number</th>
                                                    <th>Last In Time </th>
                                                    <th>Last Out Time</th>
                                                    <th>Action</th>
                                                    <th></th>
                                                </tr>
                                            </thead>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--/widget-body-->
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="myModal" tabindex="-1" role="dialog" data-backdrop="static" aria-labelledby="myModalLabel" aria-hidden="true">
 
    <div class="modal-dialog" style="width: 680px;">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="" style=""><b>Edit</b></h5>
            </div>
            <div class="modal-body">
                <table class="table table-striped table-responsive">
                    <tr>
                        <td><label>Tag ID:</label></td>
                        <td><span id="Cname"></span> </td>
                        <td style="width: 100px;"> <label>Vehicle No:</label></td>
                        <td><span id="lblCustid"></span> </td>
                    </tr>
                    @*<tr>
                        <td><label>Company Name:</label></td>
                        <td><span id="lblorderid"></span> </td>
                        <td><label>Employe:</label></td>
                        <td><span id="lblemp"></span> </td>
                    </tr>
                    <tr>
                        <td><label>Date:</label></td>
                        <td><span id="lblDate"></span> </td>
                        <td><label>Remarks:</label></td>
                        <td>
                            <input id="txtRemarks" type="text" placeholder="Remarks" onkeypress="$('#txtRemarks').css({ 'border': '', 'background': '' }); " maxlength="100" class="form-control" style="width: 200px;">
                        </td>
                    </tr>*@
                </table>

                @*<table id="tblRemarks" class="table table-striped table-responsive"></table>
                <br />
                <h5 class="innerAll border-bottom margin-bottom-none pull-left" style="margin-top:-7px;"><b>Remarks By Accounts Verified</b></h5>
                <br />
                <table id="tblAccRemarks" class="table table-striped table-responsive"></table>*@

            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" onclick="$('#txtRemarks').css({ 'border': '', 'background': '' }); $('#txtRemarks').val('')">Close</button>
                <input type="button" id="btnSearch" class="btn btn-primary" onclick="AddRemarks()" value="Add Remarks" />
            </div>
        </div>
    </div>
</div>
@section scripts{
    <script src="~/Scripts/app/common.js"></script>
    <script src="~/Scripts/app/GodrejVehList.js" @DateTime.Now.Ticks></script>
}
