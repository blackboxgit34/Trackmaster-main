<div class="modal fade" id="SendReply" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="height:343px">
    <div class="modal-dialog" id="AddNewETM">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="myModalLabel">Reply</h4>

            </div>
            <div class="modal-body">
                <table>
                    <tr>
                        <td><b>Email Address:</b></td>
                        <td>
                            <label id="lblMailId"></label>
                        </td>
                    </tr>
                    <tr>
                        <td><b>Remarks :</b></td>
                        <td><textarea rows="4" cols="50" id="txtsRemarks"> </textarea> </td>
                    </tr>
                </table>
                <input type="hidden" id="filepath" name="path">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" id="btnReply" class="btn btn-primary">Save changes</button>
            </div>
        </div>
    </div>
</div>

@section scripts{
    <script src="//cdn.datatables.net/plug-ins/1.10.13/api/fnFilterAll.js"></script>
    @*<script src="~/Content/js/APIURL.js"></script>*@
    <script src="@Url.Content("~/scripts/app/common.js")"@DateTime.Now.Ticks></script>
    <script src="~/Content/js/APIURL.js"@DateTime.Now.Ticks></script>
    <script src="@Url.Content("~/Scripts/app/LiveStatusNavkar.js")"@DateTime.Now.Ticks></script>
    <script>
        function showtraiWindow(vehname, bbid) {

            $("#ShowLocationOnMap").dialog({
                autoOpen: true,
                position: { my: "center", at: "200", of: window },
                width: 750,
                height: 500,
                resizable: true,
                title: 'Location On Map',
                modal: true,
                open: function () {

                    var url = "../CustomReport/GetLocationPartialView?vehname=" + (vehname) + "&bbid=" + bbid;
                    $(this).load(url);
                    $(".ui-dialog-titlebar-close").hide();
                },
                buttons: {
                    Close: function () {
                        $(this).dialog("close");
                    }
                }
            });
            return false;
        }

        function showDocsWindow(vehname, bbid) {


            $("#DocmentsDialog").dialog({
                autoOpen: true,
                position: { my: "center", at: "200", of: window },
                width: 750,
                height: 500,
                resizable: true,
                title: 'List of Document Uploaded for ' + vehname,
                modal: true,
                open: function () {

                    var url = "../CustomReport/GetDocumentPartialView?bbid=" + bbid;
                    $(this).load(url);
                    $(".ui-dialog-titlebar-close").hide();
                },
                buttons: {
                    Close: function () {
                        $(this).dialog("close");
                    }
                }
            });
            return false;
        }

        function showReplysWindow(bbid) {
            var vehicleName = "NA";
            var ID = "NA";
            $("#ReplyDialog").dialog({
                autoOpen: true,
                position: { my: "center", at: "200", of: window },
                width: 600,
                height: 450,
                resizable: true,
                title: 'Add Remarks & Upload Files',
                modal: true,
                open: function () {

                    var url = "../fms/ReplyVehicleRemarksPartialView?vehicleName=" + vehicleName + "&bbid=" + bbid + "&VehicleId=" + ID + "&custid=" + $custid;
                    $(this).load(url);
                    $(".ui-dialog-titlebar-close").hide();
                },
                buttons: {
                    Close: function () {
                        $(this).dialog("close");
                    }
                }
            });
            return false;
        }

        function ShowDocReply(mailid, filePath) {
            alert(mailid + filePath);
        }
    </script>
}