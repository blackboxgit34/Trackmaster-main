<div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title" id="vehname"></h4>
            </div>
            <div class="modal-body">
                <form class="row g-3" style="padding: 15px 30px 30px !important;">
                    <div class="col-md-6">
                        <h6 style="display:none" id="bbid"></h6>
                        <label for="type" class="form-label">Access Type</label>
                        <select id="type" class="form-select" style="width:100%">
                            <option selected>Choose type</option>
                            <option value="driver">Driver</option>
                            <option value="tracker">Tracker</option>
                        </select>
                    </div>
                    <div class="col-md-6">
                        <label for="valid" class="form-label">Select Validity</label>
                        <select id="valid" class="form-select">
                            <option selected>Choose days</option>
                            <option value="0">Forever</option>
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                            <option value="6">6</option>
                            <option value="7">7</option>
                            <option value="8">8</option>
                            <option value="9">9</option>
                        </select>
                    </div>
                    <div class="col-md-6">
                        <label for="name" class="form-label">Name</label>
                        <input type="text" class="form-control" id="name">
                    </div>
                    <div class="col-md-6">
                        <label for="mobile" class="form-label">Mobile</label>
                        <input type="tel" class="form-control" id="mobile">
                    </div>
                    <div class="col-12">
                        <button style="margin-left: 16px;" type="button" class="btn btn-primary" onclick="createaccess()">Create access</button>
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                </form>
            </div>
        </div>

    </div>
</div>


