@{
    ViewBag.Title = "Rajkot Fuel Details";
}

<script src="https://code.jquery.com/jquery-1.12.3.js"></script>
<script src="https://cdn.datatables.net/1.10.12/js/jquery.dataTables.min.js"></script>

<div class="row-fluid">

    <div class="widget-body ">

        <!-- Main window -->
        <div class="main_container" id="tables_page">
            @Html.Action("_ReportFilter", new { reportTitle = ViewBag.Title })
            <br />
            <div class="row-fluid" style="margin-bottom: 12px; margin-top: -37px;">
                <div class="widget-body" style="border: 0">
                    <div class="row-fluid">
                        <div id="alertss" class="widget span12">

                            <div class="widget-body  " style="background: #fff; overflow-x: auto">
                                <div id="DivVehcleCounter">
                                    <div class="box-content padded">
                                        
                                       
                                        <table id="dt_basic_Master" class="table table-bordered" cellspacing="0" width="100%">


                                            <thead>
                                                
                                                <tr>
                                                    <th>
                                                       S-No
                                                    </th>
                                                    <th>Vehicle No
                                                    </th>
                                                    <th>Vehicle Type
                                                    </th>
                                                    <th>
                                                       Fuel level in tank before moving when first ignition On
                                                    </th> 
                                                    
                                                    <th>
                                                        Fuel level in tank at the Day End 
                                                    </th>
                                               
                                                    <th>
                                                       Km Run during day
                                                    </th>

                                                   
                                                 
                                                   
                                                    <th>Diesel consumption in single day In Ltr</th>
                                                    <th>Avg of vehicle( KM/Ltr)</th>
                                                   
                                                    <th>
                                                        Fuel Filling in Tank
                                                    </th>
                                                    
                                                </tr>
                                            </thead>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
              
            </div>


       
        </div>
      
    </div>

   
</div>

<div id="ShowLocationOnMap" title="Basic dialog">
</div>
@section css{
    <style>
        .col-md-2 {
            width: 15.6%;
           
            margin-left: 6px;
            background: #fff;
            text-align: left;
        }

        .box {
            background: #dddcdc none repeat scroll 0 0;
            border: 0px solid #cdcdcd;
            border-radius: 3px;
            box-shadow: none;
            margin-bottom: 5px;
        }
    </style>
}
@section scripts{
    <script src="~/Scripts/app/common.js"></script>
    <script src="~/Scripts/app/Rajkotfueldetails.js"@DateTime.Now.Ticks></script>
   

}