@{
    ViewBag.Title = ViewBag.ReportTitle;
}
<script src="https://code.jquery.com/jquery-1.12.3.js"></script>
<script src="https://cdn.datatables.net/1.10.12/js/jquery.dataTables.min.js"></script>
<div class="row-fluid">
    <div class="widget-body ">

        <div class="main_container" id="tables_page">
            @Html.Action("_ReportFilter", "Report", new { reportTitle = "Trip Report" })
            <br />
            <div class="row-fluid" style="margin-bottom: 12px; margin-top: -37px;">
                <div class="widget-body" style="border: 0">
                    <div class="row-fluid">
                        <div id="alertss" class="widget span12">
                            <div class="widget-body clearfix" style="background: #fff; overflow-x: auto">
                                <div id="DivVehcleCounter">
                                    <div class="box-content padded">
                                        <table id="dt_basic_Master" class="table table-bordered" cellspacing="0" width="100%">
                                            <thead>
                                                <tr>
                                                    <th>Vehicle No</th>

                                                    <th>Branch</th>
                                                    <th>Zone</th>

                                                    <th>Detail</th>

                                                </tr>
                                            </thead>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!--/widget-body-->
            </div>
        </div>
    </div>
</div>

<div id="ShowLocationOnMap" title="Basic dialog">
</div>
@section css{
    <style>
        .col-md-2 {
            width: 15.6%;
            margin-left: 6px;
            background: #fff;
            text-align: left;
        }

        .box {
            background: #dddcdc none repeat scroll 0 0;
            border: 0px solid #cdcdcd;
            border-radius: 3px;
            box-shadow: none;
            margin-bottom: 5px;
        }

        .badge {
            display: inline-block;
            min-width: 40px;
            padding: 3px 7px;
            font-size: 12px;
            font-weight: bold;
            line-height: 1;
            color: #fff;
            text-align: center;
            white-space: nowrap;
            vertical-align: baseline;
            background-color: #999;
            border-radius: 10px;
        }

            .badge.badge-pink {
                background: #C86C1B;
            }

            .badge.badge-gray {
                background: #4F4F4F;
            }
    </style>
}
@section scripts{
    <script src="~/Scripts/app/common.js"></script>
    <script src="~/Scripts/app/PrismTripReport.js"></script>
}


