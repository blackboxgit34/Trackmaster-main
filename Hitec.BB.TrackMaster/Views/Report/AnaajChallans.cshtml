@{
    ViewBag.Title = "Anaaj Cchallans";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Content/css/multi-select.css" rel="stylesheet" />
<link href="~/Content/select2/select2.css" rel="stylesheet" />
<link href="~/Content/select2/select2-bootstrap.css" rel="stylesheet" />
<script src="~/Content/select2/select2.min.js"></script>




<style>
    .bigdrop {
        width: 600px !important;
    }

    .button {
        border: none;
        color: white;
        /* padding: 10px 40px; */
        /* text-align: center; */
        text-decoration: none;
        display: inline-block;
        font-size: 8px;
        margin: 4px 2px;
        cursor: pointer;
        -webkit-transition-duration: 0.4s;
        transition-duration: 0.4s;
        width: 83px !important;
    }

    .button1 {
        background-color: #8e9d13; /* Green */
        box-shadow: 0 8px 16px 0 rgba(0,0,0,0.2), 0 6px 20px 0 rgba(0,0,0,0.19);
        height: 29px;
    }


    .button3 {
        background-color: #008080; /* Green */
        box-shadow: 0 8px 16px 0 rgba(0,0,0,0.2), 0 6px 20px 0 rgba(0,0,0,0.19);
        height: 29px;
    }

    .button2 {
        box-shadow: 0 8px 16px 0 rgba(0,0,0,0.2), 0 6px 20px 0 rgba(0,0,0,0.19);
        height: 29px;
    }

        .button2:hover {
            box-shadow: 0 12px 16px 0 rgba(0,0,0,0.24),0 17px 50px 0 rgba(0,0,0,0.19);
        }

    .button1:hover {
        box-shadow: 0 12px 16px 0 rgba(0,0,0,0.24),0 17px 50px 0 rgba(0,0,0,0.19);
    }
</style>
<div class="row-fluid">
    <div class="main_container" id="tables_page">
        @Html.Action("_ReportFilter", "Report", new { reportTitle = "Anaaj Cchallans" })
        <br />
        <div class="row-fluid" style="margin-bottom: 12px; margin-top: -37px;">
            <div class="widget-body" style="border: 0">
                <div class="row-fluid">
                    <div id="alertss" class="widget span12">
                        <div class="widget-body clearfix" style="background: #fff;">
                            <div id="DivVehcleCounter">
                                <div class="box-content padded">
                                    <div class="col-md-12" style="overflow: auto;">
                                        <table class="table  table-striped table-bordered" id="dt_basic_Master" style="width:100% !important">
                                            <thead>
                                                <tr>
                                                    <th>Challan No</th>
                                                    <th>Truck No</th>
                                                    <th>Lifting Date</th>
                                                    <th>Accpeting Date</th>
                                                    <th>Source</th>
                                                    <th>Destination</th>
                                                    <th>Commodity Name</th>
                                                    <th>Packing</th>
                                                    <th>Bags</th>
                                                    <th>Approx. Distance</th>
                                                    <th>Actual Distance</th>
                                                    <th>View Route</th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section scripts{
    <script src="~/Scripts/app/common.js"></script>
    <script src="~/Scripts/app/anaajchallan.js" @DateTime.Now.Ticks></script>
}
