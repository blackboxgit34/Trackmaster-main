@{
    ViewBag.Title = " Inlet & Outlet Report";
    ViewBag.Report = "milk Lid";
}
<div class="row-fluid">
    <div class="widget-body">
        <!-- Main window -->
        <div class="main_container" id="tables_page">
            @Html.Action("_LidVerkaReportFilter", new { reportTitle = ViewBag.Title, report = ViewBag.Report })
            <br />
            <div class="row-fluid" style="margin-bottom: 12px; margin-top: -37px;">
                <div class="widget-body" style="border: 0">
                    <div class="row-fluid">
                        <div id="alertss" class="widget span12">
                            <div class="widget-body clearfix" style="background: #fff; overflow-x: auto">
                                <div id="DivVehcleCounter" style="padding: 15px;">
                                    <div class="box-content padded">
                                        <table id="dt_basic_Master" class="table table-bordered" width="100%">
                                            <thead>
                                                <tr>
                                                  
                                                    <th>Vehicle No
                                                    </th>
                                                    <th>Driver Name
                                                    </th>
                                                    <th>Let Open Count
                                                    </th>
                                                    <th>Let Close Count
                                                    </th>
                                                   <th>Duration (Day-hh:mm:ss)
                                                    </th>
                                                   
                                                    <th>Details</th>
                                                </tr>
                                            </thead>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>

                </div>
                <!--/widget-body-->
            </div>
            <!-- REPORT FOOTER HERE-->
            @*@Html.Partial("_ReportFooter")*@
        </div>
        <!-- /widget ---->
    </div>
    <!-- /row-fluid -->
</div>
<!-- /Main window -->

<div id="ShowLocationOnMap" title="Basic dialog">
</div>

@section scripts{
    <!--common js file-->
    <script src="@Url.Content("~/scripts/app/common.js")"@DateTime.Now.Ticks></script>
    <script type="text/javascript" src="@Url.Content("~/scripts/app/MilkLidReport.js?")"@DateTime.Now.Ticks></script>
}