@{
    ViewBag.Title = "RefTempDetail";
}
<script src="https://code.jquery.com/jquery-1.12.3.js"></script>
<script src="https://cdn.datatables.net/1.10.12/js/jquery.dataTables.min.js"></script>

<div class="row-fluid">
    <div class="widget-body ">
        <div class="main_container" id="tables_page">
            @Html.Action("_Custom", new { reportTitle = "Refrigerator Temperature Details" })
            <br />
            <div class="row-fluid" style="margin-bottom: 12px; margin-top: -37px;">
                <div class="widget-body" style="border: 0">
                    <div class="row-fluid">
                        <div id="alertss" class="widget span12">
                            <div class="widget-body  " style="background: #fff; overflow-x: auto">
                                <div id="DivVehcleCounter">
                                 
                                    <div class="box-content padded">
                                         
                                        <table id="dt_basic_Master" class="table table-bordered" cellspacing="0" width="100%">
                                            <thead>
                                                <tr>
                                                   <th class="hidden">Vehicle Name</th>
                                                    <th>DateTime</th>
                                                    <th>Temperature</th>
                                                    <th>Location</th>
                                                    <th>Speed</th>
                                                </tr>
                                            </thead>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section css{
    <style>
        
    #toast-container{
    height: 224px !important;
    overflow: hidden !important;
    top: 12px !important;
}



        .col-md-2 {
            width: 15.6%;
            margin-left: 6px;
            background: #fff;
            text-align: left;
        }

        .box {
            background: #dddcdc none repeat scroll 0 0;
            border: 0px solid #cdcdcd;
            border-radius: 3px;
            box-shadow: none;
            margin-bottom: 5px;
        }
    </style>
}

@section scripts{
    <script src="~/Scripts/app/common.js"></script>
    <script src="~/Scripts/app/RefTempDetail.js"></script>
}


