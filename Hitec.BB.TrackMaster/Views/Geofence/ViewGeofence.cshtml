
@{
    ViewBag.Title = "View-GeoFence";
    Layout = "~/Views/Shared/_MapLayout.cshtml";
}

            <div class="widget-body" style="border: 0">
                <div class="row-fluid">
                    <div class="widget span12">
                        <div class="widget-header">
                            <i class="fa fa-map-marker"></i>
                            <h5>View-GeoFence</h5>
                        </div>
                    </div>
                </div>
            </div>
            <!-- End of Heading part-->
<div class="row-fluid">
    <div class="span12">
        <div class="box_a">
            <div class="box_a_content">
                <iframe id="iframe" frameborder="0" src="@Url.Content("~/HTML/ViewFence.htm")" style="width: 100%; height: 550px;">
                </iframe>
            </div>
        </div>
    </div>
</div>

<script>
    $(document).ready(function () {

    
        var height = $(window).height();
        $('#iframe').css('height', height);

        var id = getUrlParameter("bbid");
        document.getElementById("iframe").src = "../../HTML/ViewFence.htm?bbid=" + id;
    
    });
</script>